<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="PackingImageButton">
        <item name="android:layout_height">match_parent</item>
        <item name="android:enabled">false</item>
        <item name="android:background">@drawable/selector_ib_up_most_bg</item>
    </style>

    <!--dialog-->
    <style name="Dialog.NoTitle" parent="Theme.AppCompat.Dialog">
        <item name="android:windowNoTitle">true</item>
    </style>

    <!--state_view-->
    <style name="RightAngleButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">56dp</item>
        <item name="android:background">@drawable/selector_button_blue</item>
        <item name="android:textSize">22.7sp</item>
        <item name="android:textColor">@color/tv_text_white</item>
    </style>
</resources>
